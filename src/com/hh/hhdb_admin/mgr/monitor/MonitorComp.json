		{
			"key": "cpu_info",
			"value": {
				"zh": "while IFS=: read NAME VALUE; do\n\n\t\t\tNAME=${NAME//[[:blank:]]/}\n\t\t\t\n\t\t\tif [ \"$NAME\" == \"modelname\" ]; then\n\t\t\t\tCPU_NAME_ARRAY[${#CPU_NAME_ARRAY[*]}]=\"$VALUE\"\n\t\t\tfi\n\t\t\tif [ \"$NAME\" == \"cpuMHz\" ]; then\n\t\t\t\tCPU_MHZ_ARRAY[${#CPU_MHZ_ARRAY[*]}]=\"$VALUE\"\n\t\t\tfi\n\t\t\tif [ \"$NAME\" == \"cachesize\" ]; then\n\t\t\t\tCPU_CACHE_SIZE_ARRAY[${#CPU_CACHE_SIZE_ARRAY[*]}]=\"$VALUE\"\n\t\t\tfi\n\t\tdone  < \"/proc/cpuinfo\"\n\t\techo \"CPU名称|CPU主频|CPU缓存\";\n\t\techo \"-------------------------\"\n\t\tfor (( c=0; c <${#CPU_NAME_ARRAY[*]}; c++ ))\n\t\t\tdo\n\t\t\t\techo \"${CPU_NAME_ARRAY[$c]}|${CPU_MHZ_ARRAY[$c]}|${CPU_CACHE_SIZE_ARRAY[$c]}\"\n\t\tdone",
				"en": "while IFS=: read NAME VALUE; do\n\n\t\t\tNAME=${NAME//[[:blank:]]/}\n\t\t\t\n\t\t\tif [ \"$NAME\" == \"modelname\" ]; then\n\t\t\t\tCPU_NAME_ARRAY[${#CPU_NAME_ARRAY[*]}]=\"$VALUE\"\n\t\t\tfi\n\t\t\tif [ \"$NAME\" == \"cpuMHz\" ]; then\n\t\t\t\tCPU_MHZ_ARRAY[${#CPU_MHZ_ARRAY[*]}]=\"$VALUE\"\n\t\t\tfi\n\t\t\tif [ \"$NAME\" == \"cachesize\" ]; then\n\t\t\t\tCPU_CACHE_SIZE_ARRAY[${#CPU_CACHE_SIZE_ARRAY[*]}]=\"$VALUE\"\n\t\t\tfi\n\t\tdone  < \"/proc/cpuinfo\"\n\t\techo \"CPU_NAME|CPU_FREQUENCY|CPU_CACHE\"\n\t\techo \"-------------------------\"\n\t\tfor (( c=0; c <${#CPU_NAME_ARRAY[*]}; c++ ))\n\t\t\tdo\n\t\t\t\techo \"${CPU_NAME_ARRAY[$c]}|${CPU_MHZ_ARRAY[$c]}|${CPU_CACHE_SIZE_ARRAY[$c]}\"\n\t\tdone"
			}
		},
		{
			"key": "cpu_util",
			"value": {
				"zh": "CPU_NUMBER=`cat /proc/cpuinfo | grep processor | wc -l`\n\t\tCPU_PERF=`mpstat -P ALL 1 1 | tail -n $(($CPU_NUMBER+1)) |  awk 'NR>0{print $2 \"|\" $3 \"|\" $5 \"|\" $11 }'`\n\t\techo \"CPU标示|用户使用率|系统使用率|空闲率\"\n\t\techo \"-------------------------\"\n\t\techo \"${CPU_PERF}\"",
				"en": "CPU_NUMBER=`cat /proc/cpuinfo | grep processor | wc -l`\n\t\tCPU_PERF=`mpstat -P ALL 1 1 | tail -n $(($CPU_NUMBER+1)) |  awk 'NR>0{print $2 \"|\" $3 \"|\" $5 \"|\" $11 }'`\n\t\techo \"CPU_FLAG|USER_USAGE_RATE|SYS_USAGE_RATE|IDLE_RATE\"\n\t\techo \"-------------------------\"\n\t\techo \"${CPU_PERF}\""
			}
		},
