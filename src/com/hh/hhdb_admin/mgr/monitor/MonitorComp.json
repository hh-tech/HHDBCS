		{
			"key": "disk_util_usage",
			"value": {
				"zh": "DISK_INFO=`df -h| awk 'NR>1{print $1 \"|\" $2 \"|\" $3 \"|\" $4 \"|\" $5 \"|\" $6}'`\n\t\techo \"设备名称|总量|使用量|可用量|使用百分比|加载点\"\n\t\techo \"-------------------------\"\n\t\techo \"$DISK_INFO\"",
				"en": "DISK_INFO=`df -h| awk 'NR>1{print $1 \"|\" $2 \"|\" $3 \"|\" $4 \"|\" $5 \"|\" $6}'`\n\t\techo \"DEVICE_NAME|TOTAL_NUM|USE_NUM|ENABLE_NUM|USE_PERCENT|LOAD_POINT\"\n\t\techo \"-------------------------\"\n\t\techo \"$DISK_INFO\""
			}
		},
		{
			"key": "disk_util_perf",
			"value": {
				"zh": "DISK_PERF=`iostat -dm| awk 'length($1)>0 && NR>3{print $1 \"|\" $2 \"|\" $3 \"M|\" $4 \"M|\" $5 \"M|\" $6 \"M\"}'`\n\t\techo \"设备名称|每秒读写数(TPS)|每秒读取量|每秒写入量|总计读取量|总计写入量\"\n\t\techo \"-------------------------\"\n\t\techo \"$DISK_PERF\"",
				"en": "DISK_PERF=`iostat -dm| awk 'length($1)>0 && NR>3{print $1 \"|\" $2 \"|\" $3 \"M|\" $4 \"M|\" $5 \"M|\" $6 \"M\"}'`\n\t\techo \"DEVICE_NAME|READ_WRITE_PER_SEC(TPS)|READ_NUM_PER_SEC|WRITE_NUM_PER_SEC|TOTAL_READ_NUM|TOTAL_WRITE_NUM\"\n\t\techo \"-------------------------\"\n\t\techo \"$DISK_PERF\""
			}
		},
		{
			"key": "networkMonitor",
