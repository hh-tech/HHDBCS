		{
			"key": "mem_util",
			"value": {
				"zh": "MEM_INFO=`free -m | awk 'NR==2 {print $2 \"M|\" $3 \"M|\" $4 \"M\"}'`\n\t\techo \"总内存|使用内存|空闲内存\"\n\t\techo \"-------------------------\"\n\t\techo \"$MEM_INFO\"",
				"en": "MEM_INFO=`free -m | awk 'NR==2 {print $2 \"M|\" $3 \"M|\" $4 \"M\"}'`\n\t\techo \"TOTAL_MEM|USE_MEM|IDLE_MEM\"\n\t\techo \"-------------------------\"\n\t\techo \"$MEM_INFO\""
			}
		},
