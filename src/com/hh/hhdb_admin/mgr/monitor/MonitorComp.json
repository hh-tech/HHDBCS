		},
		{
			"key": "eth_util",
			"value": {
				"zh": "cd /sys/class/net;\n\t\techo \"设备名称|IP地址|每秒接受字节|每秒发送字节\"\n\t\techo \"-------------------------\"\n\t\tfor FILE in *; do\n\t\t\tif [ -f \"/sys/class/net/$FILE/statistics/rx_bytes\" ];then\t\t\t\n\t\t\t\tIP_ADDRESS=`ifconfig $FILE | awk 'sub(/inet addr:/,\"\"){print $1}'`\n\t\t\t\tdeclare -i B_RX=`cat /sys/class/net/$FILE/statistics/rx_bytes`\n\t\t\t\tdeclare -i B_TX=`cat /sys/class/net/$FILE/statistics/tx_bytes`\n\t\t\t\tsleep 0.1\n\t\t\t\tdeclare -i E_RX=`cat /sys/class/net/$FILE/statistics/rx_bytes`\n\t\t\t\tdeclare -i E_TX=`cat /sys/class/net/$FILE/statistics/tx_bytes`\n\t\t\t\t\t\n\t\t\t\tdeclare -i RX=$E_RX*10-$B_RX*10\n\t\t\t\tdeclare -i TX=$E_TX*10-$B_TX*10\n\n\t\t\t\techo \"$FILE|$IP_ADDRESS|$RX|$TX\"\n\t\t\tfi\t\t\t\t\n\t\tdone",
				"en": "cd /sys/class/net;\n\t\techo \"DEVICE_NAME|IP_ADDRESS|REC_BYTE_PER_SEC|SEND_BYTE_PER_SEC\"\n\t\techo \"-------------------------\"\n\t\tfor FILE in *; do\n\t\t\tif [ -f \"/sys/class/net/$FILE/statistics/rx_bytes\" ];then\t\t\t\n\t\t\t\tIP_ADDRESS=`ifconfig $FILE | awk 'sub(/inet addr:/,\"\"){print $1}'`\n\t\t\t\tdeclare -i B_RX=`cat /sys/class/net/$FILE/statistics/rx_bytes`\n\t\t\t\tdeclare -i B_TX=`cat /sys/class/net/$FILE/statistics/tx_bytes`\n\t\t\t\tsleep 0.1\n\t\t\t\tdeclare -i E_RX=`cat /sys/class/net/$FILE/statistics/rx_bytes`\n\t\t\t\tdeclare -i E_TX=`cat /sys/class/net/$FILE/statistics/tx_bytes`\n\t\t\t\t\t\n\t\t\t\tdeclare -i RX=$E_RX*10-$B_RX*10\n\t\t\t\tdeclare -i TX=$E_TX*10-$B_TX*10\n\n\t\t\t\techo \"$FILE|$IP_ADDRESS|$RX|$TX\"\n\t\t\tfi\t\t\t\t\n\t\tdone"
			}
		},
